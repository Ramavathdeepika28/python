class Solution:
    def removeKdigits(self, num: str, k: int) -> str:
        st=[]
        for i in num:
            while st and k>0 and st[-1]>i:
                st.pop()
                k-=1
            st.append(i)
        if k>0:
            st=st[:-k]
        res="".join(st).lstrip('0')
        if res:
            return res
        else:
            return '0'
