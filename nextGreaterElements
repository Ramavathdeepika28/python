class Solution:
    def nextGreaterElements(self, nums: List[int]) -> List[int]:
        st=[]
        n=len(nums)
        ans=[-1]*n
        for i in range(2*n):
            idx=i%n
            while st and nums[st[-1]]<nums[idx]:
                prev=st.pop()
                ans[prev]=nums[idx]
            if i<n:
                st.append(idx)
        return ans
